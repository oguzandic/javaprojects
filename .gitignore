// .editorconfig

# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true

[*]

# Change these settings to your own preference
indent_style = space
indent_size = 2

# We recommend you to keep these unchanged
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

// .jshintrc
{
  "browser": true,
  "devel": true,
  "undef": true,
  "jquery": true,
  "sub": true,
  "node": true,
  "esnext": true,
  "globalstrict": true,
  "predef": ["angular",    "expect", "describe", "by", "beforeEach", "element", "browser", "it", "iit", "_", "util", "moment" ],
  "indent": 2,
  "quotmark": "double",
  "camelcase": true,
  "curly": true
}

// .jsbeautifyrc

{
  "html": {
    "indent_char": " ",
    "indent_size": 2,
    "wrap_attributes": "force",
    "wrap_attributes_indent_size": 2,
    "wrap_line_length": 100
  },
  "js": {
    "indent_char": " ",
    "indent_size": 2,
    "wrap_line_length": 100
  }
}

